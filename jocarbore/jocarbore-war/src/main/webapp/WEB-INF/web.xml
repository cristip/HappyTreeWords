<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
    	<servlet-name>userprofile</servlet-name>
    	<servlet-class>ro.infoiasi.cpa.jocarbore.users.GetUserProfileServlet</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>import</servlet-name>
    	<servlet-class>ro.infoiasi.cpa.jocarbore.admin.ImportSentencesServlet</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>leveldata</servlet-name>
    	<servlet-class>ro.infoiasi.cpa.jocarbore.users.GetLevelDataServlet</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>extraleveldata</servlet-name>
    	<servlet-class>ro.infoiasi.cpa.jocarbore.users.GetExtraLevelDataServlet</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>topjucatori</servlet-name>
    	<servlet-class>ro.infoiasi.cpa.jocarbore.users.TopPlayersServlet</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>nightlyreport</servlet-name>
    	<servlet-class>ro.infoiasi.cpa.jocarbore.admin.NightlyReportServlet</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>myprofile</servlet-name>
    	<servlet-class>ro.infoiasi.cpa.jocarbore.users.SaveUserProfileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>myprofile</servlet-name>
    	<url-pattern>/myprofile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>topjucatori</servlet-name>
    	<url-pattern>/topjucatori</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>nightlyreport</servlet-name>
    	<url-pattern>/nightlyreport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>userprofile</servlet-name>
    	<url-pattern>/userprofile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>import</servlet-name>
    	<url-pattern>/import</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
    	<servlet-name>leveldata</servlet-name>
    	<url-pattern>/leveldata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>extraleveldata</servlet-name>
    	<url-pattern>/extraleveldata</url-pattern>
    </servlet-mapping>
</web-app>
